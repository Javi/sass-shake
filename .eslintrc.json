{
  "rules": {
    "no-cond-assign": 2,
    "no-console": 0,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 0,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 2,
    "no-negated-in-lhs": 0,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "accessor-pairs": 0,
    "block-scoped-var": 2,
    "complexity": 0,
    "consistent-return": 0,
    "default-case": 2,
    "dot-notation": 2,
    "dot-location": [2,"property"],
    "eqeqeq": [2, "always"],
    "no-alert": 1,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-fallthrough": 0,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 2,
    "no-process-env": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": [2,"always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 2,
    "no-void": 2,
    "no-warning-comments": 0,
    "radix": 0,
    "wrap-iife": [2,"inside"],
    "yoda": [2,"never",{}],
    "strict": [1,"never"],
    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 0,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [2,{"hoist":"all"}],
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "callback-return": 0,
    "handle-callback-err": 0,
    "no-mixed-requires": 0,
    "no-new-require": 0,
    "no-path-concat": 0,
    "no-process-exit": 0,
    "no-restricted-modules": 0,
    "no-sync": 0,
    "consistent-this": [1,"_this"],
    "func-style": 0,
    "id-length": 0,
    "indent": 0,
    "lines-around-comment": [2,{"beforeBlockComment":true,"beforeLineComment":true}],
    "max-nested-callbacks": 0,
    "new-parens": 2,
    "newline-after-var": 0,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "quote-props": 0,
    "id-match": 0,
    "sort-vars": 0,
    "keyword-spacing": [2, {"after": true}],
    "space-in-parens": [2,"never",{}],
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "wrap-regex": 0,
    "arrow-spacing": [2,{"before":true,"after":true}],
    "no-class-assign": 2,
    "no-const-assign": 0,
    "object-shorthand": 0,
    "prefer-const": 0,
    "prefer-reflect": 0,
    "require-yield": 0,
    "max-depth": 0,
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "comma-dangle": [2,"never"],
    "no-irregular-whitespace": 2,
    "no-unexpected-multiline": 2,
    "curly": [2,"multi-line"],
    "guard-for-in": 1,
    "no-caller": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 0,
    "no-throw-literal": 0,
    "no-with": 2,
    "no-unused-vars": 2,
    "array-bracket-spacing": [2,"never",{}],
    "brace-style": [2,"1tbs",{}],
    "camelcase": [2,{"properties":"always"}],
    "comma-spacing": [2,{"after":true}],
    "comma-style": 2,
    "computed-property-spacing": [2,"never"],
    "eol-last": 2,
    "key-spacing": [2,{"afterColon":true}],
    "linebreak-style": [0,"unix"],
    "new-cap": 1,
    "no-array-constructor": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2,{"max":2}],
    "no-new-object": 2,
    "no-trailing-spaces": 2,
    "object-curly-spacing": [2,"always",{}],
    "padded-blocks": 0,
    "quotes": [2,"single"],
    "semi-spacing": [2,{"after":true}],
    "semi": [2,"always"],
    "space-before-blocks": [2,"always"],
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "spaced-comment": [2,"always",{}],
    "arrow-parens": 0,
    "constructor-super": 0,
    "generator-star-spacing": [2,"before"],
    "no-this-before-super": 0,
    "no-var": 0,
    "prefer-spread": 0
  },
  "plugins": [
    "standard"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "commonjs": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017,
    "parserOptions": {
      "ecmaFeatures": {
         "spread": true,
        "experimentalObjectRestSpread": true
      }
    }
  }
}
